    "/main/lists/:location/:latitude/:longitude":
    {
      "get":
      {
        "tags": [
          "main"
        ],
        "summary": "위치별 푸드트럭리스트",
        "description": "GPS기반 구별 푸드트럭 리스트 정보",
        "operationId": "basicInfo",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/xml",
          "application/json"
        ],
        "parameters": [
        {
          "in": "header",
          "name": "JWT token",
          "description": "사용자 인증을 위한 token",
          "required": true,
          "type": "string"
        },
        {
          "in": "query",
          "name": "location",
          "description": "위치 구분값, 1 : 강서양천영등포구로, 2 : 은평마포서대문, 3 : 종로중구용산, 4 : 도봉강북성북노원, 5 : 동대문중랑성동광진, 6 : 동작관악금천, 7 : 서초강남, 8 : 강동송파",
          "required": true,
          "type": "integer"
        },
        {
          "in": "query",
          "name": "latitude",
          "description": "사용자의 위도",
          "required": true,
          "type": "double"
        },
        {
          "in": "query",
          "name": "longitude",
          "description": "사용자의 경도",
          "required": true,
          "type": "double"
        }],
        "responses":
        {
          "200":
          {
            "description": "가까운 위치별로 10개씩 제공된 푸드트럭 정보",
            "schema":
            {
              "type": "object",
              "properties":
              {
                "status":
                {
                  "type": "string",
                  "description": "성공 알림",
                  "example": "success"
                },
                "data":
                {
                  "type": "array",
                  "items":
                  {
                    "type": "object",
                    "properties":
                    {
                      "storeID":
                      {
                        "type": "integer",
                        "description": "푸드트럭 인덱스용 푸드트럭ID",
                        "example": 13
                      },
                      "storeName":
                      {
                        "type": "string",
                        "description": "푸드트럭 이름",
                        "example": "김준영의 푸드트럭"
                      },
                      "storeImage":
                      {
                        "type": "string",
                        "description": "푸드트럭 사진 URL",
                        "example": "image url"
                      },
                      "reservationCount":
                      {
                        "type": "integer",
                        "description": "푸드트럭 예약자 현황",
                        "example": 8
                      },
                      "storeLocation":
                      {
                        "type": "string",
                        "description": "푸드트럭 위치",
                        "example": "서울시 마포구 상수동 65"
                      },
                      "storeDistance":
                      {
                        "type": "double",
                        "description": "푸드트럭까지 거리. 단위 : m or Km",
                        "example": 980
                      },
                      "storeDistanceUnit":
                      {
                        "type": "string",
                        "description": "푸드트럭까지 거리단위, 'm' or 'Km'",
                        "example": "m"
                      }
                    }
                  }
                },
                "msg":
                {
                  "type": "string",
                  "description": "푸드트럭정보 로딩 성공 메세지",
                  "example": "successful load store list"
                }
              }
            }
          },
          "500":
          {
            "description": "get connection error \nuser authorization error \nget store data error",
            "schema":
            {
              "$ref": "#/definitions/failMessage"
            }
          }
        }
      }
    },
